[run]
source = .
omit =
    */migrations/*
    */tests.py
    */test_*.py
    */*_test.py
    */venv/*
    */env/*
    */node_modules/*
    */management/commands/*
    manage.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod

[html]
directory = htmlcov

